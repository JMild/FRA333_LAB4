    <joint name="joint_eff" type="fixed">
        <parent link="link_3"/>
        <child link="end_effector"/>
        <origin rpy="1.57079632679 0 1.57079632679" xyz="0.8 0 0.0"/>
        <axis xyz="0 0 1"/>
    </joint>
    
    <link name="end_effector">
    </link>

    <xacro:include filename="example_gazebo.xacro" />

</robot>